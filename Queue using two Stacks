void StackQueue :: push(int x)
{
    s1.push(x);
}

//Function to pop an element from queue by using 2 stacks.
int StackQueue :: pop()
{
      int ans;
   while(!s1.empty()){
        s2.push(s1.top());
        s1.pop();
   }
   if(s2.empty()){
       ans = -1;
   }
   else{
   ans = s2.top();
   s2.pop();
   }
   while(!s2.empty()){
       s1.push(s2.top());
       s2.pop();
   }
   return ans;     
}
